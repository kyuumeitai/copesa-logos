{
  "name": "copesa-logos",
  "version": "2.0.6",
  "description": "Logos de La Tercera y copesa",
  "author": "kyuumeitai",
  "license": "MIT",
  "repository": "kyuumeitai/copesa-logos",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "source": "src/index.js",
  "exports": {
    ".": "./dist/index.modern.js",
    "./LaTercera": "./dist/index.js"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "microbundle --format modern --jsx React.createElement",
    "start": "microbundle watch --no-compress --format modern",
    "prepublish": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "gh-pages": "^2.2.0",
    "microbundle": "^0.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ]
}
